{
    "name": "snapt/nova-cli",
    "description": "Command line helper for Nova ADC.",
    "keywords": ["nova", "snapt", "adc", "waf", "console", "cli"],
    "homepage": "https://www.snapt.net",
    "type": "project",
    "license": "proprietary",
    "support": {
        "issues": "https://github.com/snapt/nova-cli/issues",
        "source": "https://github.com/snapt/nova-cli"
    },
    "authors": [
        {
            "name": "Snapt",
            "email": "hello@snapt.net"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^6.5.5|^7.3",
        "illuminate/http": "^8.0",
        "laminas/laminas-text": "^2.8",
        "nunomaduro/laravel-console-menu": "^3.2"
    },
    "require-dev": {
        "laravel-zero/framework": "^8.8",
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/nova-cli"]
}
